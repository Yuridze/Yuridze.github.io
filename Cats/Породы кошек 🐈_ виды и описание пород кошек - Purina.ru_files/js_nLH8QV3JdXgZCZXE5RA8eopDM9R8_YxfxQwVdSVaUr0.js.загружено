// wait until the document is ready

(function (w, $, Drupal) {	
	
    Drupal.behaviors.customnppebasetheme = {
       
      	ran:false,

        attach: function (context, settings) {
			
			// set main-container height dynamically based on menu height, so menu doesn't cut off
            var ww = 0;
			ww = document.body.clientWidth;	
          	
			
			if (ww > 768) {						
				var maxHeight = 0;
				jQuery("nav ul.navbar-nav .expanded.dropdown ul.dropdown-menu").each(function() {
					if (jQuery(this).height() > maxHeight) {
						maxHeight = jQuery(this).height();					
					}					
				});		
				maxHeight = Math.ceil(maxHeight + 15); // for bottom padding
				jQuery("#page-wrapper .main-container.container").css('min-height', maxHeight);				
			}

            // j
            if (jQuery('.nav-tabs').length > 0){
                    jQuery('.product-tabs:first').addClass('active');
                    jQuery('.product-tabs:first, .product-tabs:first a').attr('aria-expanded','true');
                    jQuery('.tab-pane:first').addClass('active');
            }

            if (jQuery('.thumb').length > 0){
                     jQuery('.thumb:first').addClass('active');
            }

            // Make items equal height

            jQuery('.equalheight').matchHeight();

            // Make items equal height in same row
            jQuery('.equalheight-row').matchHeight(true);

            // Make items equal height for titles
            jQuery('.equalheight-title').matchHeight(true);

            // Push footer to bottom for short pages

            jQuery(function($) {

                var docHeight = jQuery(window).height();
                var footerHeight = jQuery('#footer').height();
                var footerPos = jQuery('#footer').position();
                var footerTop = footerPos.top + footerHeight;

                if (footerTop < docHeight) {
                    $('#footer').css('margin-top', 10+ (docHeight - footerTop) + 'px');
                }



            });


            jQuery("#utoob").click(function(){
                    jQuery(".s-facebook").css('display','none');
                    jQuery(".s-instagram").css('display','none');
                    jQuery(".s-youtube").css('display','block');
            });

            jQuery("#insta").click(function(){
                    jQuery(".s-youtube").css('display','none');
                    jQuery(".s-instagram").css('display','block');
                    jQuery(".s-facebook").css('display','none');

            });

            jQuery("#fb").click(function(){
                    jQuery(".s-facebook").css('display','block');
                    jQuery(".s-instagram").css('display','none');
                    jQuery(".s-youtube").css('display','none');
            });



            
            // gallery js
            jQuery('.thumb-img-link').click(function(){
                    var img_url = jQuery(this).attr('data-url');
                    if(img_url){
                        jQuery('.gallery-img-container .thumb').removeClass('active');
                        jQuery(this).find('.thumb').addClass('active');
                        jQuery('.packshot-image img').attr('src',img_url);

                    }
            });

            // Replaces all registered marks as supscript if not rendered correctly
            if( !this.ran && jQuery("#page-wrapper").length > 0){
			  jQuery('*', 'body')
              .addBack()
              .contents()
              .filter(function(){
                  return this.nodeType === 3;
              })
              .filter(function(){
                  // Only match when contains 'simple string' anywhere in the text
                  return this.nodeValue.indexOf('®') != -1;
              })
              .each(function(){
              	 // If not wrapped in sup, wrap in sup
                  if(jQuery(this).parents("sup").length == 0){
                    var regex = /&amp;reg;|®|&lt;sup&gt;®&lt;\/sup&gt;/gi;
                    var output = jQuery(this).parent().html().replace(regex,'<sup>&reg;</sup>');
                    jQuery(this).parent().html(output);
                    this.ran = true;
                  }


              });


            };


        }}} (window, jQuery, Drupal));


function click_me(elementID)
{
        if(elementID)
        {       

                var x = document.getElementsByClassName("tabdata");
                var i;
                for (i = 0; i < x.length; i++) {
                    x[i].style.display = "none";
                }
                
                document.getElementById(elementID).style.display = "block";

                // end
        }
}

