    function showDetails(brand) {
        if(!$('.details-row').is(':visible')) {
            var last_block = $('.views-row').last();
            var details_div = $('<div class="details-row">');
            details_div.html(brand.find('.brand-details-hidden').clone());
            last_block.after(details_div);
        } else {
            $('.details-row').find('.brand-details-hidden').remove();
            $('.details-row').html(brand.find('.brand-details-hidden').clone());
        }
    }
    $('.brand-preview-card img').click(function(){
        showDetails($(this).parents('.brand-preview-card'));
    });

    $(document).on('click','.close',function(){
        $(this).parents('.details-row').hide();
    });
});

